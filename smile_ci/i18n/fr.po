#. module: smile_ci
#: model:mail.template,body_html:smile_ci.mail_template_build_result
msgid "<p><b><a href=\"${ctx.get('build_url', '')}\">${object.branch_id.name | safe} ${object.branch_id.branch} - Build ${object.id} - ${ctx.get('subject', '')}</a></b></p>\n"
"% if object.result in ('stable', 'unstable'):\n"
"  <ul>\n"
"    <li>${object.failed_test_ratio} failed tests</li>\n"
"    <li>${object.quality_code_count} quality errors</li>\n"
"    <li>${'{0:.2f}'.format(object.coverage_avg)}% coverage average</li>\n"
"  </ul>\n"
"% endif\n"
"\n"
"% if object.result == 'failed' and object.quality_code_count:\n"
"  <ul>\n"
"    <li>${object.quality_code_count} quality errors</li>\n"
"  </ul>\n"
"% endif\n"
"\n"
"% if ctx.get('commit_logs'):\n"
"  <p><b>Last commits:</b><br/>${ctx['commit_logs'] | safe}</p>\n"
"% endif\n"
"\n"
"% if ctx.get('build_error'):\n"
"  <p><b>Build creation failed</b></p>\n"
"  <p><em>Error message:</em></p>\n"
"  ${ctx['build_error'] | safe}\n"
"% endif\n"
"      "
msgstr "<p><b><a href=\"${ctx.get('build_url', '')}\">${object.branch_id.name | safe} ${object.branch_id.branch} - Build ${object.id} - ${ctx.get('subject', '')}</a></b></p>\n"
"% if object.result in ('stable', 'unstable'):\n"
"  <ul>\n"
"    <li>${object.failed_test_ratio} tests en échec</li>\n"
"    <li>${object.quality_code_count} erreurs de qualité de code</li>\n"
"    <li>${'{0:.2f}'.format(object.coverage_avg)}% de couverture de code</li>\n"
"  </ul>\n"
"% endif\n"
"\n"
"% if object.result == 'failed' and object.quality_code_count:\n"
"  <ul>\n"
"    <li>${object.quality_code_count} erreurs de qualité de code</li>\n"
"  </ul>\n"
"% endif\n"
"\n"
"% if ctx.get('commit_logs'):\n"
"  <p><b>Derniers commits:</b><br/>${ctx['commit_logs'] | safe}</p>\n"
"% endif\n"
"\n"
"% if ctx.get('build_error'):\n"
"  <p><b>Echec de la création du build</b></p>\n"
"  <p><em>Message d'erreur :</em></p>\n"
"  ${ctx['build_error'] | safe}\n"
"% endif\n"
"      "

